{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Session Summarizer",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["scripting", "tabs", "sidePanel", "commands", "contextMenus"],
  "side_panel": {
    "default_path": "index.html"
  },
  "host_permissions": ["http://*/*", "https://*/*"]
}
